#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

int main (){
  
  pid_t pid = fork();


  if(pid == 1){
    printf("I am the parent. My PID = %d\n", pid);

  }else if (pid == 0){
    printf("I am the child. My PID = %d\n", pid);

  }
  else{
    printf("Fork() failed!");
  }

  return 0;

}
